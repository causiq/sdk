{
  "name": "logary",
  "version": "1.0.0",
  "description": "Notify Logary on JavaScript exceptions",
  "author": "Henrik Feldt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/logary/logary-js.git"
  },
  "keywords": [
    "exception",
    "error",
    "logary",
    "fsharp",
    "logging",
    "logs",
    "monitoring"
  ],
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.6.0",
    "eslint-loader": "^1.3.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.4",
    "karma-fixture": "^0.2.2",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-jquery": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^2.1.0",
    "mocha-webpack": "^0.3.0",
    "sinon": "~1.17.3",
    "sinon-chai": "~2.8.0",
    "source-map-support": "^0.4.0",
    "string-replace-webpack-plugin": "0.0.3",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "jssha": "^2.0.2",
    "stacktrace-js": "^1.1.0"
  },
  "main": "dist/logary.js",
  "files": [
    "dist/logary.js",
    "dist/logary.js.map"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --color --display-error-details --display-reasons --optimize-minimize",
    "test": "NODE_ENV=dev mocha-webpack --webpack-config webpack-test.config.js --colors --require source-map-support/register ./test/unit/*.js",
    "test:watch": "NODE_ENV=dev mocha-webpack --webpack-config webpack-test.config.js --colors --require source-map-support/register --watch ./test/unit/*.js",
    "test:integration": "karma start --watch",
    "prepublish": "npm run build"
  }
}

{
  "name": "logary",
  "version": "0.8.0",
  "description": "Notify Logary on JavaScript exceptions and do arbitrary logging from JavaScript",
  "author": "Henrik Feldt",
  "license": "MIT",
  "homepage": "https://github.com/logary/logary-js",
  "repository": {
    "type": "git",
    "url": "git://github.com/logary/logary-js.git"
  },
  "keywords": [
    "exception",
    "error",
    "logary",
    "fsharp",
    "logging"
  ],
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "2.6.0",
    "eslint-loader": "1.3.0",
    "mocha": "^2.4.5",
    "mocha-webpack": "^0.2.0",
    "source-map-support": "^0.4.0",
    "string-replace-webpack-plugin": "0.0.3",
    "webpack": "^1.12.14"
  },
  "main": "dist/logary.js",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/logary.js",
    "dist/logary.js.map"
  ],
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "start": "WEBPACK_ENV=dev webpack --progress --color --watch",
    "test": "mocha-webpack --webpack-config webpack-test.config.js --growl --colors --require source-map-support/register --watch ./test/*.js",
    "prepublish": "WEBPACK_ENV=build webpack"
  }

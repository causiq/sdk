{
  "name": "logary",
  "version": "5.1.0-beta.0",
  "description": "A logging library for JavaScript/TypeScript for browser, server and serverless",
  "author": "Henrik Feldt",
  "license": "MIT",
  "main": "dist/logary.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://logary.tech",
  "files": [
    "dist/**/*"
  ],
  "repository": "git://github.com/logary/logary-js.git",
  "keywords": [
    "exception",
    "error",
    "logary",
    "fsharp",
    "logging",
    "logs",
    "monitoring"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.2"
  },
  "browserslist": "> 0.5%, not dead",
  "dependencies": {
    "@opentelemetry/api": "^0.4.0",
    "@types/jssha": "^2.0.0",
    "jssha": "^2.3.1",
    "rxjs": "^6.3.3"
  },
  "bugs": {
    "url": "https://github.com/logary/logary-js/issues"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "test": "jest",
    "docs": "typedoc --name 'Logary JS' --out docs --target es6 --theme minimal --mode modules src --exclude 'src/tests/**/*'",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "bump:patch": "yarn version --patch",
    "bump:minor": "yarn version --minor",
    "bump:major": "yarn version --major"
  },
  "private": false
}
